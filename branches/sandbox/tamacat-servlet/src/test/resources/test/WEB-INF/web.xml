<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>
  <display-name>test</display-name>
  <description>
    Test servlets
  </description>

  <context-param>
    <param-name>context-param1</param-name>
    <param-value>value1</param-value>
  </context-param>
  
  <servlet>
    <servlet-name>test1</servlet-name>
    <servlet-class>org.tamacat.servlet.test.SampleServlet</servlet-class>
    <init-param>
      <param-name>init-param1</param-name>
      <param-value>value1</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>test1</servlet-name>
    <url-pattern>/test1/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test1</servlet-name>
    <url-pattern>*.html</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>test2</servlet-name>
    <servlet-class>org.tamacat.servlet.test.SampleServlet</servlet-class>
    <init-param>
      <param-name>init-param2</param-name>
      <param-value>value2</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>test2</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>RequestHeaderExample</servlet-name>
    <servlet-class>RequestHeaderExample</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestHeaderExample</servlet-name>
    <url-pattern>/servlet/RequestHeaderExample</url-pattern>
  </servlet-mapping>
</web-app>
